   "source": [
    "import time\n",
    "def time_sec(n):\n",
    "    while n:\n",
    "        min, sec = divmod(n, 60)\n",
    "        print('{:02d}:{:02d}'.format(min, sec), end=\"\\r\")\n",
    "        time.sleep(1)\n",
    "        n -= 1\n",
    "\n",
    "    print('stop')"
   ]
  },
