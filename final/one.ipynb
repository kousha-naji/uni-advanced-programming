    "def is_prime(number):\n",
    "    if number == 2:\n",
    "        return True\n",
    "    if number <= 1 :\n",
    "        return False\n",
    "    if number % 2 == 0:\n",
    "        return False\n",
    "    for i in range(3, number//2, 2):\n",
    "        return False if number % 3 == 0 else continue\n",
    "    \n",
    "\n",
    "print(is_prime(5))"
   ]
